<mapper namespace="com.yonseidairy.site.mapper.HomeMapper">

    <select id="selectAllRegions" resultType="com.yonseidairy.site.dao.RegionDao">
        SELECT
		    A.CUSTNO,
		    A.CUSTNAME,
		    A.POLYGON_COORDS,
		    B.ADDR,
		    B.ADDR_1,
		    B.PHONE_MOBILE1
		FROM
		    REGION_LOCATION A,
		    CD_CUSTOMER B
		WHERE
		    B.OFFICE_ID = 'YSM'
		    AND A.CUSTNO = B.CUSTNO
    </select>

</mapper>