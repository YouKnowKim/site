package com.yonseidairy.site.dao;

import java.util.List;

import lombok.Data;

@Data
public class Region {
	private String code;
    private String name;
    private String addr;
    private String gubun;
    private String tel;
    private String gubun_type;
    private List<double[]> polygon;  // [lat, lng] 쌍의 리스트
}
